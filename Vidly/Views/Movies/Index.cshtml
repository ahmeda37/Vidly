@model IEnumerable<Vidly.Models.Movie>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@*
    COMMENT ON RAZOR
*@
<h2>Movies</h2>

<table class="table table-striped">
    <thead>
        <tr>
            <th>
                Movie
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var movie in Model)
        {
            <tr>
                <td>
                    @movie.Name
                </td>
            </tr>
        }
    </tbody>
</table>